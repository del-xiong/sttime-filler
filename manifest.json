{
  "name": "ST stTime filler",
  "version": "1.1",
  "manifest_version": 2,
  "description": "ST STTime filler",
  "author": "Del Xiong",
  "icons": { "16" : "images/icon.png",
      "48" : "images/48.png",
      "128" : "images/128.png"},
  "homepage_url": "https://github.com/del-xiong/sttime-filler",
    "permissions": [
    "tabs"
    ],
  "content_scripts": [
    {
      "matches": ["https://stapps.symphonyteleca.com/*"],
      "js": ["scripts/config.js","scripts/jq.js","scripts/stcard.js"]
    }
  ],
  "browser_action": {
    "default_icon": "images/icon.png", 
    "default_title": "ST stTime filler"
  }
}
